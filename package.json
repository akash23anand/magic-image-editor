{
  "name": "web-editor",
  "version": "1.0.0",
  "description": "Magic Image Editing Canvas with In-Browser AI Operators",
  "type": "module",
  "scripts": {
    "frontend": "vite",
    "backend": "python -m uvicorn server.main:app --port 7860 --reload",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:e2e": "playwright test",
    "setup:models": "ts-node scripts/setup-local-models.ts",
    "setup:models:coreml": "ts-node scripts/setup-local-models.ts --coreml",
    "setup:models:minimal": "ts-node scripts/setup-local-models.ts --minimal",
    "smoke:test": "ts-node scripts/smoke-tests.ts",
    "smoke:test:tool": "ts-node scripts/smoke-tests.ts --tool",
    "models:check": "ts-node scripts/smoke-tests.ts",
    "models:stats": "ts-node -e \"import('./src/models/index.js').then(m => console.log(JSON.stringify(m.getModelStats(), null, 2)))\""
  },
  "dependencies": {
    "@fal-ai/client": "*",
    "fabric": "^5.3.0",
    "onnxruntime-web": "^1.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tesseract.js": "^5.1.1"
  },
  "optionalDependencies": {
    "canvas": "^2.11.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/fabric": "^5.3.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
